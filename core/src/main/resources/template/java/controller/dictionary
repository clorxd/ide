package #{$package};

#ifStart($usespringannotation == false)#
import java.io.IOException;

import javax.servlet.ServletException;
import javax.servlet.annotation.WebServlet;
import javax.servlet.http.HttpServlet;
#ifEnd#
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

import com.alibaba.fastjson.JSONObject;
import com.teamide.bean.ResultBean;
import com.teamide.bean.Status;
import com.teamide.client.ClientHandler;
import com.teamide.client.ClientSession;
import com.teamide.exception.BaseException;
import com.teamide.param.DataParam;
import com.teamide.util.ResponseUtil;

#forStart($imports, $import)#
import #{$import};
#forEnd#

import #{$dictionary.$package}.#{$dictionary.$classname};

#ifStart($usespringannotation == true)#
@org.springframework.stereotype.Controller
public class #{$classname} {
#ifEnd#
#ifStart($usespringannotation == false)#
@WebServlet(urlPatterns = "/app/dictionary/#{$dictionary.$name}")
public class #{$classname} extends HttpServlet {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
#ifEnd#
	
	#ifStart($usespringannotation == true)#
	@org.springframework.web.bind.annotation.RequestMapping("/app/dictionary/#{$dictionary.$name}")
	protected void service(HttpServletRequest request, HttpServletResponse response) {
		invoke(request, response);
	}
	#ifEnd#
	#ifStart($usespringannotation == false)#
	@Override
	protected void service(HttpServletRequest request, HttpServletResponse response)
			throws ServletException, IOException {
		invoke(request, response);
	}
	#ifEnd#

	public void invoke(HttpServletRequest request, HttpServletResponse response) {
		Status status = Status.SUCCESS();
		status.setValue(#{$dictionary.$classname}.getOptions());
		ResponseUtil.outJSON(response, status);
	}

}
