#ifStart($only_content == false)#
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="#{$package}.#{$classname}.#{$classname}Mapper">
#ifEnd#
	<!-- #{$comment} -->
	#ifStart($customsqltype == 'SELECT_ONE')#
	<select id="#{$method_name}" parameterType="Map" resultMap="Map">
#{$content_mapper}
	</select>
	#ifEnd#
	#ifStart($customsqltype == 'SELECT_LIST')#
	<select id="#{$method_name}" parameterType="Map" resultMap="List">
#{$content_mapper}
	</select>
	#ifEnd#
	#ifStart($customsqltype == 'SELECT_PAGE')#
	<!-- #{$comment} 查询分页列表 -->
	<select id="#{$method_name}ForList" parameterType="Map" resultMap="List">
#{$content_mapper}
	</select>
	<!-- #{$comment} 统计 -->
	<select id="#{$method_name}ForCount" parameterType="Map" resultMap="Map">
#{$content_count_mapper}
	</select>
	#ifEnd#
	#ifStart($customsqltype == null)#
	<#{$execute_type} id="#{$method_name}" parameterType="Map">
#{$content_mapper}
	</#{$execute_type}>
	#ifEnd#
#ifStart($only_content == false)#
</mapper>
#ifEnd#