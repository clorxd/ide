#ifStart($only_content == false)#
<?xml version="1.0" encoding="UTF-8" ?>   
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="#{$package}.#{$classname}">
#ifEnd#
	<!-- #{$comment} -->
	#ifStart($customsqltype == 'SELECT_ONE')#
	<select id="#{$method_name}" >
#{$content_mapper}
	</select>
	#ifEnd#
	#ifStart($customsqltype == 'SELECT_LIST')#
	<select id="#{$method_name}" >
#{$content_mapper}
	</select>
	#ifEnd#
	#ifStart($customsqltype == 'SELECT_PAGE')#
	<select id="#{$method_name}" >
#{$content_mapper}
	</select>
	<!-- #{$comment} 统计 -->
	<select id="#{$method_name}ForCount" >
#{$content_count_mapper}
	</select>
	#ifEnd#
	#ifStart($customsqltype == null)#
	<execute id="#{$method_name}" >
#{$content_mapper}
	</execute>
	#ifEnd#
#ifStart($only_content == false)#
</mapper>
#ifEnd#